{
    "name": "ttree/ffmpeg",
    "type": "typo3-flow-package",
    "description": "FFMpeg TYPO3 Flow Package, an Object Oriented library to communicate with AVconv / ffmpeg",
    "keywords": [
        "video processing",
        "video",
        "audio processing",
        "audio",
        "avconv",
        "ffmpeg",
        "avprobe",
        "ffprobe"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominique Feyer",
            "email": "dfeyer@ttree.ch",
            "homepage": "http://www.ttree.ch/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "typo3/flow": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Ttree\\FFmpeg": "Classes"
        }
    },
    "extra": {
        "applied-flow-migrations": [
            "TYPO3.FLOW3-201201261636",
            "TYPO3.Fluid-201205031303"
        ]
    }
}